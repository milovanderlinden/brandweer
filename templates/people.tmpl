{{#information}}
    <div class="information">
        <h2>{{people.header}}<button class="hideInformation"><span>close</span></button></h2>
        <div>{{{people.body}}}</div>
    </div>
{{/information}}

{{#people}}
	<h2 class="main">{{header}}</h2>
    {{{body}}}
    {{#occupants}}
    	<div class="occupants">

	    	{{#each this}}
	    	{{@key}}
	    		<div class="f-container">
		    		<label class="f-label">{{type}} ({{time}})</label>
		    		<input 
		    			class="f-input" 
		    			type="number" 
		    			value="{{amount}}"
		    			placeholder=0
		    			id="{{time}}-{{type}}">
	    		</div>
	    	{{/each}}
    	</div>
    {{/occupants}}
    
{{/people}}